<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:Chatite.WPF">

    <Color x:Key="WhitePure">#ffffff</Color>
    <SolidColorBrush x:Key="WhitePureBrush" Color="{StaticResource WhitePure}" />

    <Color x:Key="WhiteLight">#dfdfdf</Color>
    <SolidColorBrush x:Key="WhiteLightBrush" Color="{StaticResource WhiteLight}" />

    <Color x:Key="WhiteDark">#cecece</Color>
    <SolidColorBrush x:Key="WhiteDarkBrush" Color="{StaticResource WhiteDark}" />

    <Color x:Key="Green">#558b6e</Color>
    <SolidColorBrush x:Key="GreenBrush" Color="{StaticResource Green}" />

    <Color x:Key="Blue">#246eb9</Color>
    <SolidColorBrush x:Key="BlueBrush" Color="{StaticResource Blue}" />

    <Color x:Key="Yellow">#f3a712</Color>
    <SolidColorBrush x:Key="YellowBrush" Color="{StaticResource Yellow}" />

    <Color x:Key="BlackLight">#606060</Color>
    <SolidColorBrush x:Key="BlackLightBrush" Color="{StaticResource BlackLight}" />

    <Color x:Key="BlackDark">#303030</Color>
    <SolidColorBrush x:Key="BlackDarkBrush" Color="{StaticResource BlackDark}" />

    <Color x:Key="BlackPure">#000000</Color>
    <SolidColorBrush x:Key="BlackPureBrush" Color="{StaticResource BlackPure}" />

</ResourceDictionary>